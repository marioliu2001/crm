<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bjpowernode.crm.settings.dao.DictionaryTypeDao">
    <insert id="insertDictionaryType">
        insert into tbl_dic_type
        values
        (#{code},#{name},#{description})
    </insert>

    <select id="findAllDictionaryType" resultType="com.bjpowernode.crm.settings.domain.DictionaryType">
        select * from tbl_dic_type
    </select>
    <select id="findByCode" resultType="com.bjpowernode.crm.settings.domain.DictionaryType">
        select * from tbl_dic_type where code = #{code}
    </select>
</mapper>
